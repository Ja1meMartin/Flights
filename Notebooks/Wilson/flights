def get_system_days(d):
    from datetime import date

    SYSTEM_START_DATE = date(2016,12,1)
    END_DATE = date(2017,6,30)
    DAYS_IN_SYSTEM = 180 #(END_DATE - SYSTEM_START_DATE)#.days()
    return ((d - SYSTEM_START_DATE) % DAYS_IN_SYSTEM).days


def get_grid_flights(flight_cache_service, outbound, destination, month):
    """
        How do we check if the flight cach is working?
    
        outbound, destination = 3-letter code + "-" + sky
        
        month is yyyy-mm
        
        output a dictionary of prices and system days
    """
    
    dates = ymsd[month]
    ret = flights_cache_service.get_grid_prices_by_date(
        market='IE',
        currency='EUR',
        locale='en-IE',
        originplace="-".join([outbound, 'SKY']),
        destinationplace="-".join([destination, 'SKY']),
        outbounddate=month).parsed
    
    dates = ymsd[month]
    prices = [item for item in ret['Dates'][1]]
    return {d:p['MinPrice'] for d,p in zip(dates,prices) if p}

api = 'ja373863706848344628128972526671'
from skyscanner.skyscanner import FlightsCache
flights_cache_service = FlightsCache(api)
feb = '2017-02'
grid = get_grid_flights(flights_cache_service, 'TLS-sky','DUB-sky', feb)

for a,b in grid.items():
    print(a,b)